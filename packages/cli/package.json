{
	"name": "nanoctl",
	"version": "0.1.14",
	"author": "Deskree Technologies Inc.",
	"license": "Apache-2.0",
	"description": "cli for nanoservice-ts",
	"engines": {
		"node": ">=18.0.0"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"bin": {
		"nanoctl": "./dist/index.js"
	},
	"files": ["dist", "node_modules"],
	"scripts": {
		"build": "rm -rf dist && npx tsc",
		"build:dev": "tsc --watch",
		"test:dev": "vitest",
		"test": "vitest run",
		"typecheck": "tsc --noEmit --incremental",
		"clean": "rm -rf dist && rm -rf node_modules",
		"install:local": "npm link",
		"uninstall:local": "npm rm --global nanoctl",
		"dev": "npm run uninstall:local && npm run install:local && npm run build:dev && nanoctl --help",
		"postbuild": "cp -r ./src/commands/monitor/static ./dist/commands/monitor/"
	},
	"keywords": ["nanoctl", "cli", "nanoservice", "nanoservice-ts"],
	"dependencies": {
		"@clack/prompts": "^0.9.1",
		"chalk": "^5.4.1",
		"commander": "^13.1.0",
		"figlet": "^1.8.0",
		"fs-extra": "^11.2.0",
		"ink": "^5.2.1",
		"picocolors": "^1.1.1",
		"posthog-node": "^4.18.0",
		"react": "^18.3.0",
		"simple-git": "^3.27.0",
		"type-fest": "^4.34.1",
		"yocto-spinner": "^0.1.1",
		"zod": "^3.24.0",
		"open": "^10.1.2",
		"serve-handler": "^6.1.6",
		"ai": "^4.3.16",
		"@ai-sdk/openai": "^1.3.22",
		"fast-glob": "^3.3.3",
		"pluralize": "^8.0.0"
	},
	"devDependencies": {
		"@types/figlet": "^1.7.0",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^22.15.21",
		"@types/react": "^19.1.5",
		"nodemon": "^3.1.9",
		"rimraf": "^6.0.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3",
		"vitest": "^3.1.4",
		"@types/pluralize": "^0.0.33"
	},
	"private": false
}
